{
    "name": "fromz/camunda-php-gen",
    "description": "Generator for the alternative Camunda PHP SDK",
    "type": "library",
    "license": "Apache 2",
    "require": {
        "thefrozenfire/swagger": "^2.0@dev",
        "nikic/php-parser": "^4.2"
    },
    "config": {
        "platform": {
            "php": "7.2"
        }
    },
    "autoload": {
        "psr-4": {
            "Gen\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    }
}
