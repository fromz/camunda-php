{
    "name": "fromz/camunda-php",
    "description": "Alternative Camunda PHP SDK",
    "type": "library",
    "license": "Apache 2",
    "require": {
        "guzzlehttp/guzzle": "~6.0",
        "symfony/serializer": "^4.2",
        "symfony/property-access": "^4.2",
        "thefrozenfire/swagger": "^2.0"
    },
    "config": {
        "platform": {
            "php": "7.2"
        }
    },
    "autoload": {
        "psr-4": {
            "Camunda\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    }
}
